    "url": "http://github.com/jeromeklam/react-bootstrap-front/issues"
  },
  "main": "dist/react-bootstrap-front.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "test": "jest --silent=false",
    "prepublish": "npm run build",
    "studio": "rekit-studio -p 8477",
    "doc:prepare": "cross-env NODE_ENV=development gitbook install",
    "doc:serve": "npm run doc:prepare && gitbook serve",
    "reactide-server": "webpack-dev-server --config reactide.config.js --mode development --hot"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.4",
    "autosize": "^4.0.2",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "core-js": "^3.6.5",
    "date-arithmetic": "^4.0.1",
    "date-fns": "^2.16.1",
    "dom-helpers": "^5.1.0",
    "draft-convert": "^2.1.8",
    "draft-js": "^0.11.4",
    "html-entities": "^1.3.1",
    "imask": "^6.0.5",
    "immutability-helper": "^2.8.1",
    "install": "^0.13.0",
    "invariant": "^2.2.4",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "loglevel": "^1.6.8",
    "memoize-one": "^5.1.1",
    "npm": "^6.14.8",
    "object-assign": "^4.1.1",
    "object.omit": "^3.0.0",
    "prop-types": "^15.7.2",
    "react-app-polyfill": "^2.0.0",
    "react-countdown-circle-timer": "^2.3.7",
    "react-datepicker": "^2.11.0",
    "react-draft-wysiwyg": "^1.14.4",
    "react-imask": "^6.0.3",
    "react-observe-size": "^0.3.0",
    "react-overlays": "^2.0.0-0",
    "react-popopo": "^2.1.9",
    "react-portal-tooltip": "^2.4.7",
    "react-sortable-hoc": "^1.10.1",
    "react-transition-group": "^4.3.0",
    "react-world-flags": "^1.3.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-logger": "^3.0.6",
    "trello-smooth-dnd": "1.0.0",
    "uncontrollable": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react-app": "7.0.2",
    "coveralls": "3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^2.1.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.13.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-hooks": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.0.0",
    "fs-file-tree": "1.0.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.0.0",
    "jest-pnp-resolver": "^1.2.2",
    "jest-resolve": "26.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.0.1",
    "striptags": "2.2.1",
    "style-loader": "^0.23.1",
    "styled-components": "4.0.3",
    "url-loader": "^2.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$",
      "node_modules/(?!imask)"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
